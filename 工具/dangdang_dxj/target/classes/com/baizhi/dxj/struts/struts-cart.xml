<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 2.3//EN" "http://struts.apache.org/dtds/struts-2.3.dtd" >
<struts>
	<package name="cart" extends="struts-default">
		
		<action name="addCart" class="com.baizhi.dxj.action.cartAction" method="addcart">
			<result name="cart" >/front/cart/cart_list.jsp</result>
		</action>
		<action name="updateCount" class="com.baizhi.dxj.action.cartAction" method="updateCount">
			<result name="updateCount">/front/cart/cart_list.jsp</result>
		</action>
		<action name="deleteCart" class="com.baizhi.dxj.action.cartAction" method="deleteCart">
			<result name="deletecart" >/front/cart/cart_list.jsp</result>
		</action>
	</package>
	
</struts>
